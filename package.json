{
  "name": "uni-preset-vue",
  "version": "0.0.10",
  "scripts": {
    "dev": "npm run dev:h5",
    "build": "npm run build:h5",
    "dev:h5": "uni",
    "dev:mp-alipay": "uni -p mp-alipay",
    "dev:mp-weixin": "uni -p mp-weixin",
    "build:h5": "uni build",
    "build:mp-alipay": "uni build -p mp-alipay",
    "build:mp-weixin": "uni build -p mp-weixin",
    "lint": "npm run tsc && npm run eslint && npm run stylelint",
    "tsc": "vue-tsc --noEmit --skipLibCheck",
    "eslint": "eslint --ext .vue,.ts src/ --fix",
    "stylelint": "stylelint \"src/**/*.(vue|scss|css)\" --fix",
    "postinstall": "husky install",
    "add": "node ./auto/addPage.ts",
    "cz": "git add . && cz",
    "tag": "npm version patch && git push && git push origin --tags"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-3070320230222002",
    "@dcloudio/uni-components": "3.0.0-3070320230222002",
    "@dcloudio/uni-mp-alipay": "3.0.0-3070320230222002",
    "@dcloudio/uni-mp-weixin": "3.0.0-3070320230222002",
    "crypto-js": "^4.2.0",
    "pinia": "2.0.33",
    "pinia-auto-refs": "0.0.10",
    "pinia-plugin-persist-uni": "1.2.0",
    "vue": "3.2.47"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.36.0",
    "@commitlint/config-conventional": "17.4.4",
    "@dcloudio/types": "3.3.2",
    "@dcloudio/uni-automator": "3.0.0-3070320230222002",
    "@dcloudio/uni-cli-shared": "3.0.0-3070320230222002",
    "@dcloudio/uni-stacktracey": "3.0.0-3070320230222002",
    "@dcloudio/vite-plugin-uni": "3.0.0-3070320230222002",
    "@vue/eslint-config-typescript": "11.0.2",
    "@vue/tsconfig": "0.1.3",
    "auto-import-types": "0.0.4",
    "autoprefixer": "10.4.14",
    "commitizen": "4.3.0",
    "commitlint": "17.4.4",
    "cz-customizable": "7.0.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "postcss-html": "1.5.0",
    "postcss-scss": "4.0.6",
    "prettier": "2.8.4",
    "sass": "1.59.2",
    "stylelint": "14.1.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.0.1",
    "typescript": "4.9.5",
    "unocss": "0.50.4",
    "unplugin-auto-import": "0.15.1",
    "unplugin-vue-components": "0.24.1",
    "vite": "4.0.3",
    "vue-global-api": "0.4.1",
    "vue-tsc": "1.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
